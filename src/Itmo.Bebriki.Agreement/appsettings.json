{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "gRPC": {
        "Url": "http://*:8082",
        "Protocols": "Http2"
      }
    }
  },
  "Infrastructure": {
    "Persistence": {
      "Postgres": {
        "Host": "localhost",
        "Port": 5438,
        "Database": "postgres",
        "Username": "postgres",
        "Password": "postgres",
        "SslMode": "Prefer"
      }
    }
  },
  "Presentation": {
    "Kafka": {
      "Host": "localhost:8001",
      "Consumers": {
        "JobTaskProcessing": {
          "Topic": "job_task_submissions",
          "BufferSize": 100,
          "BufferWaitLimit": "00:00:01",
          "ParallelismDegree": 1,
          "Group": "task-service[job_task_submissions]"
        }
      },
      "Producers": {
        "JobTaskSubmission": {
          "Topic": "job_task_decisions"
        }
      }
    }
  },
  "Platform": {
    "ServiceName": "agreement-service",
    "Observability": {
      "Logging": {
        "Serilog": {
          "MinimumLevel": {
            "Default": "Verbose",
            "Override": {
              "Microsoft.AspNetCore": "Information",
              "Grpc": "Information",
              "Npgsql": "Warning"
            }
          }
        }
      }
    }
  }
}
